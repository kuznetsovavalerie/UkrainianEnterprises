@model UkrainianEnterprises.Models.EnterpriseViewModel
@{
    ViewBag.Title = "View";
}
<div class="row">
    <h3>@Model.Title</h3>
</div>
<div class="row">
    <p>Веб-сторінка: <a href="@Model.Website" target="_blank">@Model.Website</a></p>
    <p>Номер телефону: <a href="@("tel://" + Model.PhoneNumber)">@Model.PhoneNumber</a></p>
    <p>Електронна пошта: <a href="@("mailto:" + Model.Email)">@Model.Email</a></p>
    <p>Головна адреса: @Model.Address</p>
</div>
<div class="row">
    <h5>@Model.Description</h5>
</div>

<div class="row">
    <iframe width="600" height="450" frameborder="0" style="border:0" src="@Model.GoogleMapsAPIUrl" allowfullscreen></iframe>
</div>

<div class="row">
    
</div>